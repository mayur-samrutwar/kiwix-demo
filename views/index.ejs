.overlay {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: opacity 500ms;
  visibility: hidden;
  opacity: 0;
}
.overlay:target {
  visibility: visible;
  opacity: 1;
}

.popup {
    margin: auto;
    top:25%;
  padding: 20px;
  background: #fff;
  border-radius: 5px;
  width: 40%;
  position: relative;
  transition: all 2s ease-in-out;
}

.popup .close {
  position: absolute;
  top: 20px;
  right: 30px;
  transition: all 200ms;
  font-size: 30px;
  font-weight: bold;
  text-decoration: none;
  color: #333;
}
.popup .content {
  overflow: auto;
}


.left__content{
    width: 30%;
    height: 200px;
    margin-top: 20px;
    float: left;
    position: absolute;

}

.right__content{
    margin-left: 30%;
    line-height: 130%;
    text-align: justify;
    padding: 0 20px;
}

.right__btn{
    position: relative;
    font-size: 20px;
    border: none;
    top: 25%;
    margin-top: 10px;
    color: #fff;
    width: 180px;
}
.right__btn:hover{
    cursor: pointer;
}
.download__btn{
    background-color: #090909;
    padding: 10px 30px;
}
.open__btn{
    border: 2px #090909 solid;
    padding: 8px 36px;
    background-color: #fff;
    color: #090909;
}
.tags{
    padding:20px 0;
}
.tag{
    background-color: #e7e7e7;
    color: black;
    padding:10px 5px;
    margin-right: 5px;
}

